#include <iostream>
using namespace std;
int main() 
{
    int n;
    cout<<"input:";
    cin>>n;
        for(int i=1;i<=n;i++)
        {
            for(int j=5;j>=i;j--)
            {
            cout<<"*";  
            }
            cout<<endl;
        }
    return 0;
}

